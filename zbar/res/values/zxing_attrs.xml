<?xml version="1.0" encoding="UTF-8"?>
<resources>

  <declare-styleable name="zbar_view">
    <attr name="zbar_scanner_layout" format="reference"/>
  </declare-styleable>

  <declare-styleable name="zbar_camera_preview">
    <attr name="zbar_framing_rect_width" format="dimension" />
    <attr name="zbar_framing_rect_height" format="dimension" />
    <attr name="zbar_use_texture_view" format="boolean" />
    <attr name="zbar_preview_scaling_strategy" format="enum">
      <enum name="centerCrop" value="1" />
      <enum name="fitCenter" value="2" />
      <enum name="fitXY" value="3" />
    </attr>
  </declare-styleable>

  <declare-styleable name="zbar_finder">
    <attr name="zbar_possible_result_points" format="color"/>
    <attr name="zbar_result_view" format="color"/>
    <attr name="zbar_viewfinder_laser" format="color"/>
    <attr name="zbar_viewfinder_mask" format="color"/>
  </declare-styleable>

</resources>
